{
  "env": {
      "browser": true,
      "commonjs": true,
      "es2021": true,
      "node": true
  },
  "extends": "standard",
  "parserOptions": {
      "ecmaVersion": "latest"
  },
  "rules": {
    "semi":[2, "always"],
    "indent":["error", 2],
    "prefer-const": ["error", {
      "destructuring": "any",
      "ignoreReadBeforeAssign": false
    }],
    "object-curly-spacing": ["error", "always"] 
  }
}
